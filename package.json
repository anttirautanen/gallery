{
  "name": "@anttirautanen/gallery",
  "version": "0.0.1",
  "author": "Antti Rautanen",
  "description": "Photo gallery",
  "license": "MIT",
  "keywords": [
    "gallery",
    "photos",
    "images"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/anttirautanen/gallery"
  },
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "dev": "vite",
    "typecheck": "tsc --noEmit"
  },
  "type": "module",
  "main": "./dist/gallery.umd.cjs",
  "module": "./dist/gallery.js",
  "exports": {
    ".": {
      "types": "./dist/gallery.d.ts",
      "import": "./dist/gallery.js",
      "require": "./dist/gallery.umd.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "react": ">=19",
    "react-dom": ">=19",
    "styled-components": ">=6"
  },
  "devDependencies": {
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "prettier": "3.7.4",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "styled-components": "^6",
    "typescript": "5.9.3",
    "vite": "7.3.0"
  }
}
